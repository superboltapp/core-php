{
    "name": "superbolt/core-php",
    "type": "library",
    "description": "PHP package for Superbolt",
    "keywords": ["superbolt", "core-php"],
    "homepage": "https://github.com/superbolt/core-php",
    "license": "",
    "authors": [
        {
            "name": "Superbolt",
            "email": "info@superbolt.app",
            "homepage": "https://github.com/superboltapp"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "symplify/easy-coding-standard": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Superbolt\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Superbolt\\Core\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
